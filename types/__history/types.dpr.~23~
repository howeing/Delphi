  type
    TSample<T> = record
      private
      type
        FInnerRec<R> = record
          i :R;
        end;
        TQux<T> = class
          X :Integer;
        end;
      public
        vSample :T;
        function Fun<F>(value :F) : F;
    end;

function TSample<T>.Fun<F>(value :F) : F;
begin
  result :=value;
end;

